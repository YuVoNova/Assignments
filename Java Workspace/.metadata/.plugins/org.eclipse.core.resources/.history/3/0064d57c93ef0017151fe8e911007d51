class T1
{
	public static void main(String[] args)
	{
		String x = "Java", y = "Java";
		
		if (x.matches(y))
		{
			System.out.println("Matches = true");
		}
		else 
		{
			System.out.println("Matches = false");
		}
		
		if (x.equals(y))
		{
			System.out.println("Equals = true");
		}
		else 
		{
			System.out.println("Equals = false");
		}
		
		System.out.println();
	}
}

class A
{
	public int a;
	
	public A()
	{
		
	}
	public A(int a)
	{
		this.a = a;
	}
	
	public void ABC()
	{
		
	}
}

class B extends A
{	
	public B(int b)
	{
		super();
		
		super.ABC();
	}
}

















