class T1
{
	public static void main(String[] args)
	{
		String x = "Java", y = "Java";
		
		B b = new B();
		C c = new C();
		
		Demo((A)b);
		Demo((A)c);
		
		System.out.println();
	}
	
	public static void Demo(A object)
	{
		object.ABC(5.0);
	}
}

class A
{
	public int a;
	
	public void ABC(double i)
	{
		System.out.println(i);
	}
}

class B extends A
{	
	public void ABC(double i)
	{
		System.out.println(i * 2);
	}
}

class C extends A 
{
	public void ABC(double i)
	{
		System.out.println(i * 3);
	}
}

















